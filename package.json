{
  "name": "population-trend-graph",
  "version": "0.1.0",
  "description": "This is a simple page application for population trend graph.",
  "engines": {
    "node": "20.14.0",
    "yarn": "4.2.2",
    "npm": "yarnを使用してください",
    "pnpm": "yarnを使用してください"
  },
  "packageManager": "yarn@4.2.2",
  "main": "",
  "scripts": {
    "build": "next build",
    "check-code": "run-p type-check lint spell-check",
    "dev": "next dev",
    "fix": "run-p \"fix:*\"",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings 0 --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write --ignore-path .gitignore",
    "fix:config": "prettier \"./*.{js,json}\" --write --ignore-path .gitignore",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "lint": "run-p \"lint:*\"",
    "lint-config-by-prettier": "prettier \"./*.{js,json}\" --check --cache --ignore-path .gitignore",
    "lint-editorconfig": "editorconfig-checker",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings 0 --cache",
    "lint-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --check --cache --ignore-path .gitignore",
    "lint:config": "run-s lint-config-by-prettier lint-editorconfig",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\" --gitignore --no-progress",
    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "6.1.11"
  },
  "devDependencies": {
    "@types/node": "20.14.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "cspell": "8.8.3",
    "editorconfig-checker": "5.1.5",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unicorn": "53.0.0",
    "fixpack": "4.0.0",
    "npm-run-all2": "6.2.0",
    "prettier": "3.3.0",
    "typescript": "5.4.5"
  },
  "private": true
}
